@page "/general"

<Meta property="og:title" content="Alethia Island - General Information" />

@md

@code {
    [Inject]
    HttpClient Http { get; set; }

    MarkupString md = new();

    protected override async Task OnInitializedAsync()
    {
        md = new(await Http.GetMarkdownAsHtml("https://raw.githubusercontent.com/Alethia-Island/WebLinks/gh-pages/general_information.md"));
        await base.OnInitializedAsync();
    }
}