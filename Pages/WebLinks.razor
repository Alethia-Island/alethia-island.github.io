@page "/links"

@md

@code {
    [Inject]
    HttpClient Http { get; set; }

    MarkupString md = new();

    protected override async Task OnInitializedAsync()
    {
        md = new(await Http.GetMarkdownAsHtml("https://raw.githubusercontent.com/Alethia-Island/WebLinks/gh-pages/index.md"));
        await base.OnInitializedAsync();
    }
}