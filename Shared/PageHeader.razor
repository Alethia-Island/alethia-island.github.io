<PageTitle>Alethia Island - @Title</PageTitle>

<p class="pt-2"><h2><i class="@Icon me-2" aria-hidden="true"></i>@(Title ?? SubTitle)</h2></p>

@code {
    [Parameter]
    public string? Icon { get; set; }
    
    [Parameter]
    public string? SubTitle { get; set; }

    [Parameter]
    public string? Title { get; set; }
}